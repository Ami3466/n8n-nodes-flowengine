{
  "name": "n8n-nodes-flowengine",
  "version": "1.10.12",
  "description": "n8n nodes for FlowEngine AI + Zero-Setup Email Testing + AI Session Manager + Data Cleaner - AI automation made easy",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "flowengine",
    "ai",
    "automation",
    "email",
    "testing",
    "smtp",
    "ethereal",
    "langchain",
    "openai",
    "anthropic",
    "session",
    "uuid",
    "agent",
    "memory",
    "data-cleaner",
    "deduplicate",
    "fuzzy",
    "phone",
    "normalize"
  ],
  "license": "MIT",
  "homepage": "https://flowengine.cloud",
  "author": {
    "name": "FlowEngine",
    "email": "support@flowengine.cloud"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ami3466/n8n-nodes-flowengine.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build",
    "postinstall": "rm -rf node_modules/imapflow/test node_modules/imapflow/examples node_modules/imapflow/node_modules 2>/dev/null || true"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FlowEngineApi.credentials.js",
      "dist/credentials/FlowEngineLlmApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/FlowEngine/FlowEngine.node.js",
      "dist/nodes/FlowEngineLlm/FlowEngineLlm.node.js",
      "dist/nodes/SendEmailTest/SendEmailTest.node.js",
      "dist/nodes/AiSessionManager/AiSessionManager.node.js",
      "dist/nodes/DataCleaner/DataCleaner.node.js"
    ]
  },
  "devDependencies": {
    "@types/mailparser": "^3.4.6",
    "@types/nodemailer": "^7.0.4",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@langchain/openai": "^0.3.14"
  },
  "optionalDependencies": {
    "imapflow": "^1.1.1",
    "mailparser": "^3.9.0",
    "nodemailer": "^7.0.10"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "overrides": {
    "imapflow": {
      "nodemailer": "$nodemailer"
    }
  }
}
